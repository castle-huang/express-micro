							"raw": "{\r\n  \"id\": \"string\",\r\n  \"avatarUrl\": \"avatar\",\r\n  \"fullName\": \"bb\",\r\n  \"phoneCode\": \"+11\",\r\n  \"phone\": \"1234\",\r\n  \"email\": \"bb@gmail.com\"\r\n}",
