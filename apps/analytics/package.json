{
  "name": "@sojo-micro/analytics",
  "version": "1.0.0",
  "description": "Example usage of TS RPC  framework",
  "scripts": {
    "build": "tsc",
    "start": "node dist/app.js",
    "dev": "ts-node-dev --respawn --transpile-only --watch src app.ts",
    "pre-vercel-build": "cd ../../ && npm install && npm run build",
    "vercel-build": "npm run pre-vercel-build"
  },
  "dependencies": {
    "@sojo-micro/rpc": "^1.0.0",
    "dotenv": "^17.2.3",
    "express": "^5.1.0"
  },
  "devDependencies": {
    "@types/dotenv": "^6.1.1",
    "@types/node": "^20.19.5",
    "ts-node": "^10.9.1",
    "ts-node-dev": "^2.0.0"
  }
}
